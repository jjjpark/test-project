<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--   에러시  window-preferences-maven검색-Download Artifact javadoc 체크할것 -->


<mapper namespace="com.example.demo.dao.UserDao">

	<select id="getUserList"
		resultType="com.example.demo.dto.UserDto">
		SELECT * FROM loluser
	</select>


	<!-- 회원가입 메소드 -->
	<insert id="insertUser"
		parameterType="com.example.demo.dto.UserDto">
    <![CDATA[
       INSERT INTO loluser (username, userpwd,ireum, nickname, gender)
      VALUES (#{username}, #{userpwd}, #{ireum}, #{nickname}, #{gender}) 
     ]]>
	</insert>
</mapper>